package controlstatements;
import java.util.Scanner;
public class ForLoopPr3Table {
	public static void main(String[] args)
	{
		int i;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number : ");   
		int k=sc.nextInt();
		
		for(i=1;i<=10;i++)
		{
			System.out.println(k*i);
			i++;
		}
	}

}